<!doctype html>
<html lang="en">
<head>
    <title>Abak Tools</title>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge, chrome=1">
    <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=0">

    <partial name="_CoreStyles" />

    @RenderSection("Styles", required: false)

    <!-- Favicon -->
    <link rel="shortcut icon" href="~/dist/assets/images/favicon.png">
</head>
<body class="layout-default">
    <!-- WRAPPER -->
    <div id="wrapper">

        <!-- NAVBAR -->
        <partial name="_NavBar" />
        <!-- END NAVBAR -->
        <!-- LEFT SIDEBAR -->
        <vc:navigation-menu />
        <!-- END LEFT SIDEBAR -->
        <!-- MAIN -->
        <div class="main">

            <!-- MAIN CONTENT -->
            @RenderBody()
            <!-- END MAIN CONTENT -->
            <!-- RIGHT SIDEBAR -->
            <partial name="_RightSidebar" />
            <!-- END RIGHT SIDEBAR -->

        </div>
        <!-- END MAIN -->

        <div class="clearfix"></div>

        <!-- footer -->
        <footer>
            <div class="container-fluid">
                <p class="copyright">&copy; 2021 <a href="epox.dev" target="_blank">epox.dev</a>. All Rights Reserved.</p>
            </div>
        </footer>
        <!-- end footer -->

    </div>
    <!-- END WRAPPER -->
    <partial name="_CoreScripts" />
    @RenderSection("Scripts", required: false)
</body>
</html>